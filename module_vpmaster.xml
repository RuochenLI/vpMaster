      <src refid="vpmaster.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.vpmaster"/>
    </javac2>
    
    <copy todir="${vpmaster.output.dir}">
      <fileset dir="${module.vpmaster.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.vpmaster.basedir}/src/main/resources">
